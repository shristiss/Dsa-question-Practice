class Solution {
public:
    int firstUniqChar(string s) {
        unordered_map<char,int>mp;
        for(int i=0;i<s.length();i++)
        {
            if(mp.count(s[i])==0)mp[s[i]]=1;
            else mp[s[i]]++;
        }
        for(int i=0;i<s.length();i++)
        {
            if(mp[s.at(i)]==1)return i;
        }
        return -1;
    }
};
